journal = {Ann Biomed Eng},
keywords = { Biological; Particle Size; Pulmonary Ventilation, Inhalation; Aerosols, administration /\&/ dosage/pharmacokinetics; Bronc, metabolism/physiology/radiography, metabolism/physiology; Bronchography; Cadaver; Co, methods; Trachea, physiology/radiography; Models, physiology; Respiratory Physiological Phenomena; , physiology; Rheology,Administration},
month = apr,
number = {4},
pages = {374--390},
pmid = {12723679},
title = {{Computational fluid dynamics simulation of airflow and aerosol deposition in human lungs.}},
