journal = {Computer methods in biomechanics and biomedical engineering},
keywords = {Biomechanics,Computer Simulation,Humans,Image Processing, Computer-Assisted,Models, Anatomic,Models, Biological,Radiographic Image Interpretation, Computer-Assist,Respiratory Mechanics,Respiratory System,Respiratory System: anatomy \& histology,Respiratory System: radiography,Tomography, X-Ray Computed},
month = aug,
number = {4},
pages = {267--77},
