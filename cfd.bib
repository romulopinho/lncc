institution = {Department of Mathematics, University of Auckland, Auckland, New Zealand.},
journal = {Biophys J},
keywords = { Anatomic; Models, Chemical; Models, Smooth, Smooth Muscle, Statistical; Models, Theoretical; Muscle Contraction; Muscle, chemistry; Humans; Models, metabolism; Phosphorylation; Pulmonary Artery, pathology, pathology; Myocytes, pathology; Myosin-Light-Chain Phosphatase, pathology; Trachea,Animals; Calcium},
month = mar,
number = {6},
pages = {2053--2064},
pmid = {18065464},
title = {{A mathematical model of airway and pulmonary arteriole smooth muscle.}},
url = {http://dx.doi.org/10.1529/biophysj.107.113977},
volume = {94},
year = {2008}
}
