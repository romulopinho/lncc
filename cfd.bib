institution = {University Hospital Antwerp, Department of Pulmonology, Belgium. Jan.DeBacker@ua.ac.be},
journal = {Med Eng Phys},
keywords = { Computer-Assisted; Female; Forced Expiratory Volu, Theoretical; Pressure; Pulmonary Ventilation, X-Ray Computed, anatomy /\&/ histology/pathology; Male; Middle Age, methods, methods; Computer Simulation; Diagnosis, methods; Software; Tomography, physiology; Radionuclide Imaging,Aged; Computational Biology},
month = sep,
number = {7},
pages = {872--879},
pmid = {18096425},
title = {{Flow analyses in the lower airways: patient-specific model and boundary conditions.}},
url = {http://dx.doi.org/10.1016/j.medengphy.2007.11.002},
volume = {30},
year = {2008}
}
